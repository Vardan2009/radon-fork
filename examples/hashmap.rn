# HashMap data type in Radon

var hashmap = {
    "name": "John",
    "age": 23,
    "address": "123 Main St"
}
print(hashmap)

var value = hashmap["name"]
print(value)

hashmap["bcd"] = 234
hashmap["abc"] = 456

print(hashmap)

while true {
    var key = input("Enter key: ")
    if key == "exit" {break}
    print(hashmap[key])
}

